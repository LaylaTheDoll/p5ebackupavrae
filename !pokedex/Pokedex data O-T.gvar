	"Serperior":
	{
		"id": "497", "type1": "Grass", "type2": null, "sr": "13", "image": "https://cdn.bulbagarden.net/upload/b/b7/497Serperior.png", "desc": "They raise their heads to intimidate opponents but only give it their all when fighting a powerful opponent."
	},
	"Servine":
	{
		"id": "496", "type1": "Grass", "type2": null, "sr": "5", "image": "https://cdn.bulbagarden.net/upload/7/73/496Servine.png", "desc": "They avoid attacks by sinking into the shadows of thick foliage. They retaliate with masterful whipping techniques."
	},
	"Seviper":
	{
		"id": "336", "type1": "Poison", "type2": null, "sr": "5", "image": "https://cdn.bulbagarden.net/upload/d/d6/336Seviper.png", "desc": "Seviper shares a generations-long feud with Zangoose. The scars on its body are evidence of vicious battles. This Pokémon attacks using its sword-edged tail."
