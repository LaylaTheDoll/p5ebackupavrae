	"Sewaddle":
	{
		"id": "540", "type1": "Bug", "type2": "Grass", "sr": "0.5", "image": "https://cdn.bulbagarden.net/upload/4/4a/540Sewaddle.png", "desc": "This Pokémon makes clothes for itself. It chews up leaves and sews them with sticky thread extruded from its mouth."
	},
	"Sharpedo":
	{
		"id": "319", "type1": "Water", "type2": "Dark", "sr": "10", "image": "https://cdn.bulbagarden.net/upload/a/a8/319Sharpedo.png", "desc": "Sharpedo can swim at speeds of up to 75 mph by jetting seawater out of its backside. This Pokémon's drawback is its inability to swim long distances."
	},
	"Shaymin Land":
	{
		"id": "492", "type1": "Grass", "type2": null, "sr": "15", "image": "https://cdn.bulbagarden.net/upload/0/05/492Shaymin-Land.png", "desc": "It lives in flower patches and avoids detection by curling up to look like a flowering plant."
	},
	"Shaymin Sky":
	{
		"id": "492", "type1": "Grass", "type2": "Flying", "sr": "15", "image": "https://cdn.bulbagarden.net/upload/d/da/492Shaymin-Sky.png", "desc": "It lives in flower patches and avoids detection by curling up to look like a flowering plant."
	},
	"Shedinja":
	{
		"id": "292", "type1": "Bug", "type2": "Ghost", "sr": "1", "image": "https://cdn.bulbagarden.net/upload/5/59/292Shedinja.png", "desc": "Shedinja's hard body doesn't move-not even a twitch. In fact, its body appears to be merely a hollow shell. It is believed that this Pokémon will steal the spirit of anyone peering into its hollow body from its back."
