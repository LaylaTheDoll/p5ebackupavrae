	"Shellos":
	{
		"id": "422", "type1": "Water", "type2": null, "sr": "2", "image": "https://cdn.bulbagarden.net/upload/7/72/422Shellos.png", "desc": "Its colors and shapes differ from region to region. In the Sinnoh region, two types are confirmed."
	},
	"Shelmet":
	{
		"id": "616", "type1": "Bug", "type2": null, "sr": "1", "image": "https://cdn.bulbagarden.net/upload/f/f6/616Shelmet.png", "desc": "It evolves when bathed in an electric-like energy along with Karrablast. The reason is still unknown."
	},
	"Shieldon":
	{
		"id": "410", "type1": "Rock", "type2": "Steel", "sr": "3", "image": "https://cdn.bulbagarden.net/upload/e/e2/410Shieldon.png", "desc": "A Pokémon that lived in jungles around 100 million years ago. Its facial hide is extremely hard."
	},
	"Shiftry":
	{
		"id": "275", "type1": "Grass", "type2": "Dark", "sr": "9", "image": "https://cdn.bulbagarden.net/upload/f/f7/275Shiftry.png", "desc": "Shiftry is a mysterious Pokémon that is said to live atop towering trees dating back over a thousand years. It creates terrific windstorms with the fans it holds."
	},
	"Shiinotic":
	{
		"id": "756", "type1": "Grass", "type2": "Fairy", "sr": "8", "image": "https://cdn.bulbagarden.net/upload/thumb/3/36/756Shiinotic.png/600px-756Shiinotic.png", "desc": "It emits flickering spores that cause drowsiness. When its prey succumb to sleep, this Pokémon feeds on them by sucking in their energy."
	},
	"Shinx":
	{
		"id": "403", "type1": "Electric", "type2": null, "sr": "0.25", "image": "https://cdn.bulbagarden.net/upload/3/32/403Shinx.png", "desc": "All of its fur dazzles if danger is sensed. It flees while the foe is momentarily blinded."
	},
	"Shroomish":
	{
		"id": "285", "type1": "Grass", "type2": null, "sr": "0.5", "image": "https://cdn.bulbagarden.net/upload/d/d8/285Shroomish.png", "desc": "If Shroomish senses danger, it shakes its body and scatters spores from the top of its head. This Pokémon's spores are so toxic, they make trees and weeds wilt."
	},
	"Shuckle":
	{
		"id": "213", "type1": "Bug", "type2": "Rock", "sr": "4", "image": "https://cdn.bulbagarden.net/upload/c/c7/213Shuckle.png", "desc": "Shuckle quietly hides itself under rocks, keeping its body concealed inside its hard shell while eating berries it has stored away. The berries mix with its body fluids to become a juice."
