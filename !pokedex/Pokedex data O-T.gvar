	"Sinistea":
	{
		"id": "854", "type1": "Ghost", "type2": null, "sr": "???", "image": "https://cdn.bulbagarden.net/upload/b/b1/854Sinistea.png", "desc": "The swirl pattern in this Pokémon’s body is its weakness. If it gets stirred, the swirl loses its shape, and Sinistea gets dizzy."
	},
	"Sirfetch'd":
	{
		"id": "865", "type1": "Fighting", "type2": null, "sr": "9", "image": "https://cdn.bulbagarden.net/upload/thumb/3/38/865Sirfetch%27d.png/600px-865Sirfetch%27d.png", "desc": "After deflecting attacks with its hard leaf shield, it strikes back with its sharp leek stalk. The leek stalk is both weapon and food."
	},
	"Sizzlipede":
	{
		"id": "850", "type1": "Fire", "type2": "Bug", "sr": "0.5", "image": "https://cdn.bulbagarden.net/upload/f/f2/850Sizzlipede.png", "desc": "It stores flammable gas in its body and uses it to generate heat. The yellow sections on its belly get particularly hot."
	},
	"Skarmory":
	{
		"id": "227", "type1": "Steel", "type2": "Flying", "sr": "10", "image": "https://cdn.bulbagarden.net/upload/3/35/227Skarmory.png", "desc": "he wing feathers it sheds can be processed and made into knives whose sharpness is recognized by the finest chefs."
	},
	"Skiddo":
	{
		"id": "672", "type1": "Grass", "type2": null, "sr": "1", "image": "https://cdn.bulbagarden.net/upload/thumb/5/5d/672Skiddo.png/250px-672Skiddo.png", "desc": "Thought to be one of the first Pokémon to live in harmony with humans, it has a placid disposition."
	},
	"Skiploom":
	{
