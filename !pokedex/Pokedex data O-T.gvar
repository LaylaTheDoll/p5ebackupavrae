	"Skuntank":
	{
		"id": "435", "type1": "Poison", "type2": "Dark", "sr": "8", "image": "https://cdn.bulbagarden.net/upload/b/bc/435Skuntank.png", "desc": "It sprays a vile-smelling fluid from the tip of its tail to attack. Its range is over 160 feet."
