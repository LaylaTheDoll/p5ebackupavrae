	"Shuppet":
	{
		"id": "353", "type1": "Ghost", "type2": null, "sr": "1", "image": "https://cdn.bulbagarden.net/upload/4/4b/353Shuppet.png", "desc": "Shuppet grows by feeding on dark emotions, such as vengefulness and envy, in the hearts of people. It roams through cities in search of grudges that taint people."
	},
	"Sigilyph":
	{
		"id": "561", "type1": "Psychic", "type2": "Flying", "sr": "10", "image": "https://cdn.bulbagarden.net/upload/6/67/561Sigilyph.png", "desc": "The guardians of an ancient city, they use their psychic power to attack enemies that invade their territory."
	},
	"Silcoon":
	{
		"id": "266", "type1": "Bug", "type2": null, "sr": "1", "image": "https://cdn.bulbagarden.net/upload/e/ef/266Silcoon.png", "desc": "Silcoon tethers itself to a tree branch using silk to keep from falling. There, this Pokémon hangs quietly while it awaits evolution. It peers out of the silk cocoon through a small hole."
	},
	"Silicobra":
	{
		"id": "843", "type1": "Ground", "type2": null, "sr": "???", "image": "https://cdn.bulbagarden.net/upload/b/b5/843Silicobra.png", "desc": "As it digs, it swallows sand and stores it in its neck pouch. The pouch can hold more than 17 pounds of sand."
	},
	"Silvally":
	{
		"id": "773", "type1": "Normal", "type2": null, "sr": "14", "image": "https://cdn.bulbagarden.net/upload/thumb/b/be/773Silvally.png/600px-773Silvally.png", "desc": "This is its form once it has awakened and evolved. Freed from its heavy mask, its speed is greatly increased."
	},
	"Simipour":
	{
		"id": "516", "type1": "Water", "type2": null, "sr": "10", "image": "https://cdn.bulbagarden.net/upload/8/83/516Simipour.png", "desc": "The high-pressure water expelled from its tail is so powerful, it can destroy a concrete wall."
	},
	"Simisage":
	{
		"id": "512", "type1": "Grass", "type2": null, "sr": "10", "image": "https://cdn.bulbagarden.net/upload/2/24/512Simisage.png", "desc": "It attacks enemies with strikes of its thorn-covered tail. This Pokémon is wild tempered."
	},
	"Simisear":
	{
		"id": "514", "type1": "Fire", "type2": null, "sr": "10", "image": "https://cdn.bulbagarden.net/upload/7/7c/514Simisear.png", "desc": "A flame burns inside its body. It scatters embers from its head and tail to sear its opponents."
