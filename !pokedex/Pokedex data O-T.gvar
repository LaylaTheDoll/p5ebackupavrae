	"Shelgon":
	{
		"id": "372", "type1": "Dragon", "type2": null, "sr": "7", "image": "https://cdn.bulbagarden.net/upload/a/a5/372Shelgon.png", "desc": "Covering Shelgon's body are outgrowths much like bones. The shell is very hard and bounces off enemy attacks. When awaiting evolution, this Pokémon hides away in a cavern."
	},
	"Shellder":
	{
		"id": "90", "type1": "Water", "type2": null, "sr": "0.5", "image": "https://cdn.bulbagarden.net/upload/4/40/090Shellder.png", "desc": "The shell can withstand any attack. However, when it is open, the tender body is exposed."
