	"Slowbro":
	{
		"id": "80", "type1": "Water", "type2": "Psychic", "sr": "11", "image": "https://cdn.bulbagarden.net/upload/8/80/080Slowbro.png", "desc": "Lives lazily by the sea. If the Shellder on its tail comes off, it becomes a Slowpoke again."
